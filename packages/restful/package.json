{
  "name": "@kagari/restful",
  "version": "0.4.7",
  "module": "",
  "main": "dist/index.js",
  "repository": "https://github.com/kagari-project/kagari",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./browser": {
      "import": "./dist/browser.js",
      "require": "./dist/browser.js",
      "types": "./browser.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "browser": [
        "dist/browser"
      ]
    }
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "joi": "^17.6.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.5.0"
  },
  "devDependencies": {
    "jest": "^29.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "29.0.3",
    "typescript": "~4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "gitHead": "251a3bb77317b7366768548ec2e0234f1a8fe1a8"
}
