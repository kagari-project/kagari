{
  "root": true,
  "env": {
    "node": true,
    "jest": true
  },
  "plugins": [
    "html"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "react-app",
    "react-app/jest",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "react-hooks/exhaustive-deps": "off",
    "no-unreachable": "error"
  }
}