{
  "name": "@kagari/restful",
  "version": "0.6.2",
  "module": "",
  "main": "dist/index.js",
  "repository": "https://github.com/kagari-project/kagari",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./browser": {
      "import": "./dist/browser.js",
      "require": "./dist/browser.js",
      "types": "./browser.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "browser": [
        "dist/browser"
      ]
    }
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "joi": "^17.6.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.5.0"
  },
  "devDependencies": {
    "rimraf": "^5.0.5",
    "typescript": "~4.7.4"
  },
  "gitHead": "ff71fdb3635644d30e178cc76c522771461be81c"
}
