{
  "name": "kagari",
  "version": "1.0.0",
  "private": true,
  "packageManager": "yarn@3.4.1",
  "scripts": {
    "postinstall": "husky install",
    "lint": "eslint '{apps,packages,components}/**/src/**.{ts,tsx}'",
    "prettier": "prettier --write '{apps,packages,components}/**/src/**'",
    "changelog": "lerna version --conventional-commits --amend -y",
    "build:all": "yarn workspaces foreach run build",
    "prepublish": "yarn build:all && yarn changelog",
    "publish": "lerna publish",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "workspaces": {
    "packages": [
      "packages/**",
      "apps/**",
      "components/ui"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@storybook/addon-essentials": "^7.0.2",
    "@storybook/addon-interactions": "^7.0.2",
    "@storybook/addon-links": "^7.0.2",
    "@storybook/blocks": "^7.0.2",
    "@storybook/react": "^7.0.2",
    "@storybook/react-webpack5": "^7.0.2",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/jest": "^29.2.3",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "babel-loader": "^8.3.0",
    "eslint": "8.37.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-html": "^7.1.0",
    "husky": "^8.0.1",
    "jest": "^29.3.1",
    "lerna": "^6.1.0",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.2",
    "typescript": "~4.7.4"
  }
}
